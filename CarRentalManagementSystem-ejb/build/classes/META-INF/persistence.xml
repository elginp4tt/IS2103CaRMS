<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="CarRentalManagementSystem-ejbPU" transaction-type="JTA">
    <jta-data-source>jdbc/carRentalManagementSystem</jta-data-source>
    <class>entity.CarCategoryEntity</class>
    <class>entity.CarEntity</class>
    <class>entity.CarModelEntity</class>
    <class>entity.CarPickupEntity</class>
    <class>entity.CarReturnEntity</class>
    <class>entity.CustomerEntity</class>
    <class>entity.DispatchEntity</class>
    <class>entity.EmployeeEntity</class>
    <class>entity.OutletEntity</class>
    <class>entity.PartnerEntity</class>
    <class>entity.RentalRateEntity</class>
    <class>entity.ReservationEntity</class>
    <class>entity.TransitDriverEntity</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
